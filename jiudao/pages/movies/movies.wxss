
.header{
  position: fixed;
  z-index: 99;
  background: white;
  height: 100rpx;
  width: 100%;
  border-top:1px solid #f5f5f5;
  border-bottom:1px solid #f5f5f5;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  box-shadow:0 0 3px 0 #e3e3e3;
}
.header icon{
  margin-right:10rpx;
}
.box{
  height: 68rpx;
  width: 700rpx;
  background: #f5f5f5;
  border-radius: 50px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.box .placeholder{
  font-size: 28rpx;
  text-align: center;
}
.container{
  padding-top: 100rpx;
  background-color: #f2f2f2;
}
.loading{
  position: absolute;
  top: 50%;
  left:50%
}
